@model Hadia.Models.DomainModels.Mem_EducationalQualificationMaster
@{
    ViewData["Title"] = "Edit";
}

<h2>Edit</h2>
<form asp-action="Edit" method="post">
    <div class="col-md-4">
        <div class=" form-group">
            @Html.LabelFor(x => x.DegreeName)
            @Html.TextBoxFor(x => x.DegreeName)
        </div>
        <div class=" form-group">
            @Html.LabelFor(x => x.Rank)
            @Html.TextBoxFor(x => x.Rank)
        </div>
        <button class="btn btn-success">Save</button>
    </div>
</form>
