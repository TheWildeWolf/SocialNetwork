
@model List<Hadia.Models.ViewModels.EducationalQualificationMasterViewModel>
@{
    ViewData["Title"] = "Index";

}
<div class="card">

    <div class="card-header bg-white header-elements-inline">
        <h6 class="card-title">Educational Qualifications</h6>
        <div class="header-elements">
            <a asp-action="Create" class="btn bg-blue">Create New</a>
        </div>
    </div>



    <div class="card-body">
        <table class="table">
            <thead>
                <tr>
                    <th>
                        Sn
                    </th>
                    <th>
                        Degree Name
                    </th>
                    <th>
                        Rank
                    </th>
                    <th>
                        Is Phd
                    </th>
                    <th>
                        Option
                    </th>
                </tr>
            </thead>
            <tbody>
                @{ int SlNo = 0; }
                @foreach (var master in Model)
                {
                    <tr>
                        <td> @(SlNo += 1)</td>
                        <td>@Html.DisplayFor(x => master.DegreeName)</td>
                        <td>@Html.DisplayFor(x => master.Rank)</td>
                        <td>@Html.DisplayFor(x => master.IsPhd)</td>
                        <td>
                            <a asp-action="Edit" asp-route-id="@master.Id"class="btn btn-outline bg-primary border-primary text-primary-800 btn-icon"><i class="icon-compose"></i></a>
                            <a asp-action="Details" asp-route-id="@master.Id"class="btn btn-outline bg-success border-primary text-primary-800 btn-icon"><i class="icon-file-eye"></i></a>
                            <a asp-action="Delete" asp-route-id="@master.Id" class="btn btn-outline bg-danger border-primary text-primary-800 btn-icon confirm"><i class="icon-trash-alt"></i></a>
                        </td>
                    </tr>
                }
                
            </tbody>
        </table>
        </div>
    </div>
