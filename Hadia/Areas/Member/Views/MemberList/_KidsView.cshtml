 @model Hadia.Models.ViewModels.MemberDetailsViewModel
        
                      <div class="card-body">
                          <table class="table table-framed">
                       <thead>
                           <tr>
                               <th>
                                   SNo
                               </th>
                               <th>
                                   Kid Name
                               </th>
                               <th>
                                   Age
                               </th>
                               <th>
                                   Gender
                               </th>
                           </tr>
                       </thead>
                       <tbody>
                           @{ int SNo = 0; }
                           @foreach (var Kid in Model.Kids)
        {

        <tr>
             <td> @(SNo += 1)</td>
         <td>@Html.DisplayFor(x =>Kid.KidName)</td>
         <td>@Html.DisplayFor(x =>Kid.Age )</td>
         <td>@Html.DisplayFor(x =>Kid.Gender)</td>
         <td>
             <a asp-action="EditKids" asp-route-id="@Kid.Id" class="btn btn-outline bg-primary border-primary text-primary-800 btn-icon"><i class="icon-compose"></i></a>
        </tr>

        }
                       </tbody>
                </table>
                      </div>
      